package bodypart;

public class BodyPart {
	private String PartName;
	private int NoOfExercises;
	public String getPartName() {
		return PartName;
	}
	public void setPartName(String bodyPartName) {
		PartName = bodyPartName;
	}
	public int getNoOfExercise() {
		return NoOfExercises;
	}
	public void setNoOfExercise(int noOfExercise) {
		NoOfExercises = noOfExercise;
	}
	
	@Override
	public String toString() {
		final StringBuilder sb = new StringBuilder("BodyPart{"); 
		sb.append("PartName=").append(PartName);
		sb.append(", NoOfExercise=").append(NoOfExercises).append('\'');
		sb.append('}');
		return sb.toString();
	}
	
}
